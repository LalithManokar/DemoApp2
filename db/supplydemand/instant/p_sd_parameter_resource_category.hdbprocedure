PROCEDURE "SAP_TM_TRP"."sap.tm.trp.db.supplydemand.instant::p_sd_parameter_resource_category" (
	IN RESOURCE_FILTER_ID BIGINT,
	OUT RESOURCE_CATEGORY VARCHAR(20)
) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA SAP_TM_TRP
	READS SQL DATA AS
BEGIN

	SELECT  RESOURCE_CATEGORY INTO RESOURCE_CATEGORY FROM "sap.tm.trp.db.filter::t_equipment_filter" WHERE ID = :RESOURCE_FILTER_ID;
	
END;