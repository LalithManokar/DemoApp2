PROCEDURE "SAP_TM_TRP"."sap.tm.trp.db.archive::p_archive_update_execution_log" ( 
IN EXECUTION_LOG_ID BIGINT,
IN STATUS TINYINT,
IN RECORD_COUNT BIGINT,
IN MESSAGE NVARCHAR(1000)) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "SAP_TM_TRP"
	AS
BEGIN

UPDATE "sap.tm.trp.db.archive::t_archive_execution_log" 
SET STATUS=:STATUS,
RECORD_COUNT=:RECORD_COUNT,
MESSAGE=:MESSAGE,
END_TIME=CURRENT_UTCTIMESTAMP
WHERE ID=:EXECUTION_LOG_ID;

END;
