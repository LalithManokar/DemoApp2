PROCEDURE "SAP_TM_TRP"."sap.tm.trp.db.archive::p_archive_update_execution_detail" ( 
IN EXECUTION_ID BIGINT,
IN STATUS TINYINT,
IN RECORDCOUNT BIGINT,
IN MESSAGE NVARCHAR(1000)) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA SAP_TM_TRP
	AS
BEGIN

UPDATE "sap.tm.trp.db.archive::t_archive_execution_detail"
SET STATUS=:STATUS,MESSAGE=:MESSAGE,END_TIME=CURRENT_UTCTIMESTAMP, RECORD_COUNT = :RECORDCOUNT
WHERE ID=:EXECUTION_ID;

END;
