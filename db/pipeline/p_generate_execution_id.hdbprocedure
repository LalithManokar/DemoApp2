PROCEDURE "SAP_TM_TRP"."sap.tm.trp.db.pipeline::p_generate_execution_id" (
OUT EXEC_ID BIGINT,
OUT EXEC_TIMESTAMP TIMESTAMP)
    LANGUAGE SQLSCRIPT
    SQL SECURITY INVOKER
    DEFAULT SCHEMA SAP_TM_TRP
    AS
BEGIN

    SELECT "sap.tm.trp.db.pipeline::s_plan_execution".NEXTVAL INTO EXEC_ID FROM DUMMY;
    SELECT CURRENT_UTCTIMESTAMP INTO EXEC_TIMESTAMP FROM DUMMY;
    
END;
