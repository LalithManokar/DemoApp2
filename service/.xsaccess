        "source": "/planningcockpit/simulations\\.(json|xml|html|txt)/(\\w+)/scenarios/(\\w+)/compositepaths/?([\\w\\{\\}\\/]+)?\\??(.+)?",
        "target": "/planningcockpit/paths.xsjs?planId=$2&scenarioId=$3&type=COMPOSITE&format=$1&raw=$4&$5"
