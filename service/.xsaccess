{
    "exposed": true,
    "prevent_xsrf": true,
    "default_connection" : "sap.tm.trp.service.config::TechnicalUser",
    "authentication": {
        "method": "Basic"
    },
    "headers": {
        "enabled": true,
        "customHeaders": [{
            "name":"X-Frame-Options",
            "value":"DENY"
        }]
    },
    "rewrite_rules": [{
        "source": "/user/settings\\.(json|xml|html|txt)\\??(.+)?",
        "target": "/user/users.xsjs?format=$1&raw=/settings&$2"
    }, {
        "source": "/user/profile\\.(json|xml|html|txt)([\\w\\{\\}\\/]+)?\\??(.+)?",
        "target": "/user/users.xsjs?format=$1&raw=/updateMyProfile&$3"
    }, {
        "source": "/planningcockpit/simulations\\.(json|xml|html|txt)/(\\w+)/scenarios/(\\w+)/paths/(\\w+)/rules/?([\\w\\{\\}\\/]+)?\\??(.+)?",
        "target": "/planningcockpit/rules.xsjs?planId=$2&scenarioId=$3&pathId=$4&format=$1&raw=$5&$6"
