PROCEDURE "SAP_TM_ROUTING_TEST"."sap.tm.trp.routing.tests.db::p_get_network_config_info_stub" (
    IN network_model_id BIGINT,
    IN usage VARCHAR(50),
    OUT network_config_info "SAP_TM_ROUTING"."sap.tm.trp.routing.db.common::tt_option"
)
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
   DEFAULT SCHEMA SAP_TM_ROUTING_TEST
   READS SQL DATA AS
BEGIN
   /*************************************
       Write your procedure logic 
   *************************************/
   DECLARE lowest_network_id BIGINT;
   
   CALL "SAP_TM_ROUTING"."sap.tm.trp.routing.db.path::p_get_network_chain"(:network_model_id, network_chain);
   
   SELECT LAST_VALUE(id ORDER BY level) INTO lowest_network_id
   FROM :network_chain;
   
   network_config_info = 
   SELECT name, value
   FROM "sap.tm.trp.routing.tests.db::t_network_conf_test"
   WHERE network_model_id = :lowest_network_id AND usage = :usage;
END