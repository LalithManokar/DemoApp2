PROCEDURE "SAP_TM_ROUTING_TEST"."sap.tm.trp.routing.tests.db::p_query_trip_name_stub" (
    IN trip_ids TABLE (trip_id NVARCHAR(100), path_id BIGINT),
    OUT trip_id_name_map TABLE(trip_id NVARCHAR(100), trip_name NVARCHAR(100))
)
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
   DEFAULT SCHEMA SAP_TM_ROUTING_TEST
   READS SQL DATA AS
BEGIN
   /*************************************
       Write your procedure logic 
   *************************************/
   trip_id_name_map = 
   SELECT A.trip_id, B.trip_name
   FROM :trip_ids AS A LEFT OUTER JOIN "sap.tm.trp.routing.tests.db::t_trip_map_test" AS B
   ON A.trip_ID = B.trip_id AND A.path_id = B.path_id;
END